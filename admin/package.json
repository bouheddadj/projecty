{
  "version": "1.0.0",
  "description": "My webpack project",
  "name": "webpack-project",
  "scripts": {
    "prepare:env:prod": "cp .env.production .env",
    "prepare:env:dev": "cp .env.development .env",
    "serve": "npm run prepare:env:dev && webpack serve",
    "build": "npm run prepare:env:prod && webpack --mode=production",
    "build:dev": "npm run prepare:env:dev && webpack --mode=development",
    "watch": "webpack --watch",
    "test": "jasmine"
  },
  "devDependencies": {
    "@types/jasmine": "^5.1.8",
    "@types/leaflet": "^1.9.17",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^6.0.3",
    "css-loader": "^7.1.2",
    "dotenv": "^16.5.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jasmine": "^5.7.1",
    "mini-css-extract-plugin": "^2.9.2",
    "style-loader": "^4.0.0",
    "supertest": "^7.1.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "node-fetch": "^3.3.2"
  }
